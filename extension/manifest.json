{
    "manifest_version": 2,
    "name": "uBlock Stats Collector",
    "version": "1.0",
    "description": "Collects statistics from uBlock Origin",
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "<all_urls>",
      "storage",
      "http://127.0.0.1:5000/*",
      "http://localhost:5000/*"
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
  }
  